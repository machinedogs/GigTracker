{
  "expo": {
    "name": "GigTracker",
    "slug": "GigTracker",
    "platforms": ["ios", "android"],
    "version": "1.0.4",
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "orientation": "portrait",
    "icon": "./assets/current.png",
    "splash": {
      "ldpi": "./assets/splash.png",
      "mdpi": "./assets/splash.png",
      "hdpi": "./assets/splash.png",
      "xhdpi": "./assets/splash.png",
      "xxhdpi": "./assets/splash.png",
      "xxxhdpi": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#212121"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.machinedogs.current",
      "config": {
        "googleMapsApiKey": "AIzaSyABVhqSwWjctOOPzSAWNFmzCzkgEbLZvjY"
      }
    },
    "android": {
      "package": "com.machinedogs.current",
      "versionCode": 1,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "VIBRATE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDv5ieMrtNv9UAeTGPiAjisrjjV5uIx-ko"
        }
      }
    }
  }
}
